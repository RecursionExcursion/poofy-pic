{
  "name": "Poofy Pic",
  "description": "Give your pics some poof",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab", "storage"],
  "icons": {
    "16": "icons/image3.png",
    "48": "icons/image3.png",
    "128": "icons/image3.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/main.js"]
    }
  ]
}
